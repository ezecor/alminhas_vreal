var alminhas={"type": "FeatureCollection","name": "vreal","crs": { "type": "name", "properties": { "name": "urn:ogc:def:crs:OGC:1.3:CRS84" } },"features": [{ "type": "Feature", "properties": { "name": "Arroios, Rua do Bairro de São João", "DESCRIÇÃO DO ORATÓRIO": "Alminhas em monólito de granito, tipo nicho, assente num muro. Núcleo emoldurado de formato retangular e topo redondo, encimado por cruz latina relevada. Protege a edícula uma porta metálica trabalhada com motivos geométricos e uma cruz trilobada ao centro.", "FICHA INVENTÁRIO": "0042-VIR-VR", "FREGUESIA": "Arroios", "LAT": "41.292833", "LONG": "-7.709358", "OBS": "https://docs.google.com/document/d/e/2PACX-1vQNHtzjpo7I4uHG6kkTxdNRjU7XQn3jBXp2eLazHlu42SyU5-T9ri9FJd6vVlcS-mwiPwFYb8cpwcxm/pub", "PAINEL": "Painel em metal, policromado, em baixo-relevo, com a representação central da Nossa Senhora do Carmo, com o Menino ao colo, que segura os escapulários em ambas as mãos. Abaixo, dois anjos auxiliam as almas no fogo do Purgatório.", "TIPOLOGIA": "", "gx_media_links": "https://doc-0s-58-mymaps.googleusercontent.com/untrusted/hostedimage/nsq40isde5crh2epv50pnah6f0/egf3eb2o4fkibbhs58212l75to/1687517429000/3evDZZ2F1ONbZ8Mo7WuI6QKm2vmYGVzF/07330417508546578562/5ACtvi-GcUU2QSThH3cIvoaC0mfmW_OzPCSQv9lsYX4edM1Gk15nH5eMjlignawvx-Xri98ztDgj5_WEI17woVoL4Tbr7tQcW-tNTLjMTQMDLbrPHOyJTWv3Nw5_0xULeyJKRGJGQQCg2UjTypJXqKYECqSwtUSUUUXpWv9nfUh_jZj_5UTxuGUBK-M4K2XcBoEHtinrf?session=0&fife https://doc-0o-58-mymaps.googleusercontent.com/untrusted/hostedimage/nsq40isde5crh2epv50pnah6f0/vp9pbibre9dhq6lhduulishr8k/1687517429000/3evDZZ2F1ONbZ8Mo7WuI6QKm2vmYGVzF/07330417508546578562/5ACtvi-H6p47bKwpHOcjpHlvWQn0bxriz_-VXItFBeBodq0tFa9slVHRImtDty4PDK7Yh4aVHgdGOLxP0udydB77o2Mkz5wClLp2zNIxX0LhujuZFB-lKocLpkrUK3L3xd1mjd0KPAclGJpR52YUsvrChn9tWjgPMfnrwPJrl1CXctRAdKpX-nlRgv2ow2cmNHT22pj6O?session=0&fife" }, "geometry": { "type": "Point", "coordinates": [ -7.709358, 41.292833, 0.0 ] } },{ "type": "Feature", "properties": { "name": "Torneiros, Ru